# FinSight Environment Variables Example
# Copy this file to .env and fill in your API keys

# LLM API
GEMINI_PROXY_API_KEY=
GEMINI_PROXY_API_BASE=

# Stock Data APIs
ALPHA_VANTAGE_API_KEY=
FINNHUB_API_KEY=
MASSIVE_API_KEY=
IEX_CLOUD_API_KEY=
TIINGO_API_KEY=
TWELVE_DATA_API_KEY=
MARKETSTACK_API_KEY=
EODHD_API_KEY=

# Search APIs
TAVILY_API_KEY=
EXA_API_KEY=

# Symbol Lookup
OPENFIGI_API_KEY=

# Macro Data
FRED_API_KEY=

# Monitoring
LANGSMITH_API_KEY=
ENABLE_LANGSMITH=false
LANGSMITH_PROJECT=FinSight

# Email Alerts (Optional)
SMTP_SERVER=
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
EMAIL_FROM=

# Scheduler
PRICE_ALERT_SCHEDULER_ENABLED=false
NEWS_ALERT_SCHEDULER_ENABLED=false
